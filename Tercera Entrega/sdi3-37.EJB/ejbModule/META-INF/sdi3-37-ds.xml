<?xml version="1.0" encoding="UTF-8"?>
<datasources xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://www.jboss.org/ironjacamar/schema"
    xsi:schemaLocation="http://www.jboss.org/ironjacamar/schema 
    https://docs.jboss.org/ironjacamar/schema/datasources_1_0.xsd">
	<datasource
		jndi-name="java:jboss/datasources/NotaneitorDS"
		enabled="true"
		use-java-context="true"
		pool-name="NotaneitorDS">

		<connection-url>jdbc:hsqldb:hsql://localhost/localDB</connection-url>
		<driver>hsqldb.jar</driver>
		<pool></pool>
		<security>
			<user-name>sa</user-name>
			<password></password>
		</security>
	</datasource>
</datasources>